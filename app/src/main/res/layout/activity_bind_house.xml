        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="@dimen/dp_71"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_153"
        android:layout_marginTop="@dimen/dp_14"
        android:text="@string/bindhouse"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_17"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="@dimen/dp_130"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_93"
        android:text="@string/attestation"
        android:textColor="#ff999999"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="@dimen/dp_113"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/sp_16"
        android:layout_marginTop="@dimen/dp_138"
        android:text="@string/truename"
